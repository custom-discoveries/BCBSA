CREATE QUERY findState(String state) {
/**
 Copyright (c) 2024, Custom Discoveries Inc.
 All rights reserved.
**/
  Start = {State.*};
  STRING stateSearch  = upper(state);
  RS= {};

  IF (state != "") THEN
    RS = Select s FROM Start:s WHERE s.alias == stateSearch;
  ELSE
    RS = Select s FROM Start:s;
  END;

  PRINT RS, RS.size();
}